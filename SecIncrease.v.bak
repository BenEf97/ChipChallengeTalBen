module SecIncrease(CarryInSec, OutSec,ovflwSec);
input CarryInSec;
output reg [5:0] OutSec;
output reg ovflwSec;

reg [5:0] counter;
localparam MaxSec = 60;

always @(posedge CarryInSec) begin

	if(counter==MaxSec-1) begin
		counter <= 0;
		ovflwSec <= 1;
	end
	else begin
		counter <= counter +1;
		ovflwSec <=0;
	end
end

assign Outsec = counter;

endmodule